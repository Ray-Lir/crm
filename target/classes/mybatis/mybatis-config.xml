<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--配置别名-->
    <typeAliases>
        <package name="com.ray.crm.settings.domain"/>
        <package name="com.ray.crm.workbench.domain"/>
    </typeAliases>

    <!--配置sql映射文件-->
    <mappers>
        <package name="com.ray.crm.settings.dao"/>
        <package name="com.ray.crm.workbench.dao"/>
    </mappers>

</configuration>
